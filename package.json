{
  "name": "pixlr",
  "version": "1.0.0",
  "description": "Pixlr lets people manipulate a 64x64 Led panel via a web app",
  "main": "server.js",
  "scripts": {
    "build": "browserify -t [ babelify --presets [ react ] ] -t [ envify --NODE_ENV production  ] ./client/app.jsx --outfile ./public/js/app.js",
    "build-stage": "browserify -t [ babelify --presets [ react ] ] -t [ envify --NODE_ENV development  ] ./client/app.jsx --outfile ./public/js/app.js",
    "start": "node server.js",
    "prepare": "git clone --recursive git@github.com:phuongvu/pixlr.git node_modules"
  },
  "keywords": [
    "Raspberry Pi 2",
    "Physical Web",
    "NodeJS",
    "ReactJS",
    "Websocket"
  ],
  "author": "Phuong Vu <me@phuong.vu>",
  "license": "ISC",
